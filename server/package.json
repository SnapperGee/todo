{
  "name": "todo-server",
  "version": "0.0.1",
  "description": "Server backend of todo.",
  "main": "./build/dist/js/index.js",
  "type": "module",
  "private": true,
  "authors": [
    "Danny Collan",
    "Zachary Guitierrez",
    "Maria Adame",
    "Snap <XxSnapperGeexX@gmail.com> (https://github.com/SnapperGee)"
  ],
  "scripts": {
    "build": "tsc -p ./tsconfig.dist.json",
    "dev": "NODE_ENV=development node --enable-source-maps --loader ts-node/esm ./src/main/ts/index.ts",
    "drop": "NODE_ENV=development node --loader ts-node/esm ./scripts/drop.ts",
    "prebuild": "rm -rf ./build/dist",
    "prestart": "npm run build",
    "seed": "NODE_ENV=development node --loader ts-node/esm ./scripts/seed.ts",
    "start": "NODE_ENV=production node ./build/dist/js/index.js",
    "ts" : "tsc -p ./tsconfig.dev.json"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "mongoose": "^8.1.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "dotenv": "^16.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
